{
  "name": "react-starter",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development REACT_FAST_REFRESH=true webpack serve --hot",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --display=errors-only",
    "format": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --noUnusedLocals",
    "ts": "tsc",
    "verify": "yarn typecheck && yarn lint",
    "bundle-analyzer": "cross-env NODE_ENV=production webpack --BUNDLE_ANALYZER"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn typecheck"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/cache": "11.0.0",
    "@emotion/react": "11.1.1",
    "@emotion/styled": "11.0.0",
    "axios": "0.21.0",
    "final-form": "4.20.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-final-form": "6.5.2",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-react": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@emotion/babel-plugin": "11.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@svgr/webpack": "5.5.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@typescript-eslint/parser": "4.8.2",
    "babel-loader": "8.2.1",
    "babel-plugin-emotion": "11.0.0",
    "cross-env": "7.0.2",
    "css-loader": "5.0.1",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "prettier": "2.2.0",
    "react-refresh": "0.9.0",
    "style-loader": "2.0.0",
    "typescript": "4.1.2",
    "webpack": "5.6.0",
    "webpack-bundle-analyzer": "4.1.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0"
  }
}
